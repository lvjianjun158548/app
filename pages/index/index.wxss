@import '/public/fontfal/fontfal.wxss';
.nav {
  width: 750rpx;
  height: 300rpx;
}

.icon {
  width: 100rpx;
  height: 100rpx;
}
.icon-list {
  height: 240rpx; 
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
}
.icon-list view {
  display: flex;
  flex-direction: column;
}
/* 列表 */
.list {
  margin-top: 20rpx;
  background: #fff;
}
.list-nav {
  display: flex;
  justify-content: space-between;
  padding: 0 10rpx;
}
.list-item{
  display: flex;
  margin-top: 10rpx;
}
.list-item-left {
  width: 420rpx;
  height: 186rpx;
}
.list-item-left image{
  width: 100%;
  height: 100%;
}

.list-item-right {
  font-size: 12px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.list-item-right view:last-child {
  display: flex;
  justify-content: space-between;
}
